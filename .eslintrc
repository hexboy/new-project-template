{
  "extends": ["jquery", "gulp"],
  "rules": {
    "quotes": ["error", "single"],
    "space-in-parens": ["error", "never"],
    "space-after-keywords": "off",
    "keyword-spacing": ["error", {"before": true,"after": true}]
  },
  "globals": {
    "jQuery": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  }
}
